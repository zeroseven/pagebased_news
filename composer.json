{
	"name": "zeroseven/pagebased-news",
	"description": "Create news articles as “normal” pages in TYPO3. Easy to install and ready to use straight away.",
	"license": "GPL-2.0-only",
	"type": "typo3-cms-extension",
	"authors": [
		{
			"name": "zeroseven design studios GmbH",
			"email": "typo3@zeroseven.de",
			"homepage": "https://www.zeroseven.de/",
			"role": "Company"
		},
		{
			"name": "Raphael Thanner",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^8.0 | ^8.1",
		"typo3/cms-core": "^13.4",
		"zeroseven/pagebased": "*",
		"doctrine/dbal": "~4.0.0"
	},
	"conflict": {
        "doctrine/dbal": "^4.1.0"
	},
	"autoload": {
		"psr-4": {
			"Zeroseven\\PagebasedNews\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "pagebased_news"
		}
	}
}
